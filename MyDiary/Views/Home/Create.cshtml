@model MyDiary.Models.Diary
@{
                /**/

                ViewBag.Title = "Create";
}
@using (Html.BeginForm())
{
    @Html.ValidationSummary(false)
    <div class="form-group">
        @Html.LabelFor(m => m.Title)
        @Html.TextBoxFor(m=>m.Title,new { @class="form-control",placeholde="请输入文章标题"})
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Content)
        @Html.TextBoxFor(m => m.Content, new { @class = "form-control", placeholde = "请输入文章内容" })
    </div>
    <p class="text-right">
        <button type="submit" class="btn btn-primary">创建</button>
    </p>
}

